@{
    ViewData["Title"] = "Home Page";
    
}


<div class="text-center">
    <h1 class="display-4">Welcome,  @TempData["EmployeeName"]?.ToString() </h1>
    <a class="btn btn-primary" asp-controller="Home" asp-action="Employe">EMPLOYEE LIST</a>
    @if (User.IsInRole("Admin"))
    {
        <a class="btn btn-primary" asp-controller="Home" asp-action="CreateEmployee">Add Employee</a>
    }
</div>
